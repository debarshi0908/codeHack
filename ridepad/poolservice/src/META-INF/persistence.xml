<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="poolservices">
	<class>org.ridepad.entity.User</class>
	<class>org.ridepad.entity.Location</class>
	<class>org.ridepad.entity.Address</class>
	<class>org.ridepad.entity.Car</class>
	<class>org.ridepad.entity.Group</class>
	<class>org.ridepad.entity.Reqtran</class>
	<class>org.ridepad.entity.Carmaster</class>
		<properties>
			<property name="eclipselink.logging.level" value="FINE"/>
			<property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
			<property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/ridepad?charSet=latin1"/>
			<property name="eclipselink.jdbc.password" value="admin"/>
			<property name="eclipselink.jdbc.user" value="root"/>
		</properties>
	</persistence-unit>
</persistence>
